            <geom density="1056" fromto="0 0 -0.0525 0 0 -0.1875" name="left_lower_arm" size="0.04" />
            <site class="touch" name="left_lower_arm" pos="0 0 -0.1" size="0.041 0.0685" zaxis="0 0 1" />

            <body name="left_hand" pos="0 0 -0.204">
