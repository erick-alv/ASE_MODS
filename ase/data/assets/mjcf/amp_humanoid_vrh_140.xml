              <site class="touch" name="left_hand" size=".041" type="sphere" />
              
              <body name="left_controller" pos="0.08 0 0">
                <site name="left_controller" pos="-0.04 0 0" size="0.04 0.01 0.01" type="box" />
              </body>
            </body>
          </body>
        </body>
      </body>

      <body name="right_thigh" pos="0 -0.084887 0">
        <site class="force-torque" name="right_hip" />
        <joint armature=".02" axis="1 0 0" damping="50" name="right_hip_x" range="-60 15" stiffness="500" />
        <joint armature=".02" axis="0 1 0" damping="50" name="right_hip_y" range="-140 60" stiffness="500" />
        <joint armature=".02" axis="0 0 1" damping="50" name="right_hip_z" range="-60 35" stiffness="500" />
        <geom density="1269" fromto="0 0 -0.056 0 0 -0.337" name="right_thigh" size="0.055" />
        <site class="touch" name="right_thigh" pos="0 0 -0.1965" size="0.056 0.28200000000000003" zaxis="0 0 -1" />

        <body name="right_shin" pos="0 0 -0.394">
