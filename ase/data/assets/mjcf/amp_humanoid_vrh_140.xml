          <site class="touch" name="head" pos="0 0 0.164" size="0.103" type="sphere" />
          <camera fovy="80" name="egocentric" pos="0.103 0 0.164" xyaxes="0 -1 0 .1 0 1" />
          <body name="headset" pos="0.155 0 0.185">
            <site name="headset" pos="-0.03 0 0" size="0.03 0.07 0.04" type="box" />
          </body>

        </body>

        <body name="right_upper_arm" pos="-0.02405 -0.18311 0.228">
          <joint armature=".02" axis="1 0 0" damping="40" name="right_shoulder_x" range="-180 45" stiffness="400" />
          <joint armature=".02" axis="0 1 0" damping="40" name="right_shoulder_y" range="-180 60" stiffness="400" />
          <joint armature=".02" axis="0 0 1" damping="40" name="right_shoulder_z" range="-90 90" stiffness="400" />
          <geom density="982" fromto="0 0 -0.05 0 0 -0.23" name="right_upper_arm" size=".045" />
          <site class="touch" name="right_upper_arm" pos="0 0 -0.14" size="0.046 0.1" zaxis="0 0 1" />

          <body name="right_lower_arm" pos="0 0 -0.216">
